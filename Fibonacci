Algoritmo "Fibonacci"
Var
   n, cta, v1, v2, v3:inteiro
Inicio
   Escreval ("Digite o N:")
   leia (n)
   cta <- 0
   v1 <- -1
   v2 <- 1
   v3 <- 1
   enquanto cta<n faÃ§a
            v3 <- v1 + v2
            v1 <- v2
            v2 <- v3
            escreval (v3)
            cta <- cta + 1
   fimEnquanto

Fimalgoritmo
